{
  "name": "mediboard",
  "version": "1.0.0",
  "description": "Plateforme Open Source pour les Métiers de la Santé",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "engines": {
    "node": ">= 12.18.3"
  },
  "scripts": {
    "build": "webpack --progress",
    "build-installation": "webpack --progress --config installation/vue/webpack.config.js",
    "build-installation-dev": "webpack --progress --config installation/vue/webpack-dev.config.js",
    "build-vue": "webpack --config dev/WebPack/webpack.js",
    "build-tamm": "webpack --config modules/oxCabinet/vue/_src/conf/webpack.js",
    "build-vue-dev": "npm-run-all build-vue-dev:*",
    "build-tamm-dev": "webpack --config modules/oxCabinet/vue/_src/conf/webpack-dev.js",
    "build-vue-ci": "webpack --config dev/WebPack/webpack-ci.js",
    "test-vue": "jest --config dev/WebPack/jest.config.js",
    "test-vue-output": "jest --config dev/WebPack/jest.config.js --ci --silent --outputFile",
    "test-vue-dev": "jest --config dev/WebPack/jest.config.js --watch --verbose true --no-cache",
    "test-vue-coverage": "jest --config dev/WebPack/jest.config.js --testResultsProcessor=\"jest-junit\" --collect-coverage --ci --silent --outputFile ",
    "test-vue-ci": "jest --config dev/WebPack/jest.config.js --ci --testResultsProcessor=\"jest-junit\"",
    "test-vue-spec": "jest --config dev/WebPack/jest.config.js --watch --verbose true --no-cache -t",
    "test-vue-clear": "jest --config dev/WebPack/jest.config.js --clear-cache",
    "storybook": "start-storybook --port 6006 --config-dir javascript/.storybook/ --no-manager-cache",
    "build-storybook": "npm-run-all build-storybook:*",
    "build-storybook:make-build": "build-storybook --output-dir tmp/.storybook --config-dir javascript/.storybook/",
    "build-storybook:move-build": "node javascript/.storybook/prepare-build.js",
    "build-vue-dev:storybook": "npm run build-storybook",
    "build-vue-dev:vue": "webpack --progress --config dev/WebPack/webpack-dev.js"
  },
  "author": "openxtrem",
  "license": "GPL-3.0",
